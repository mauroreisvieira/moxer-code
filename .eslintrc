env:
   node: true
   shared-node-browser: true
extends: 'plugin:prettier/recommended'
rules:
	prettier/prettier:
		- error
   indent:
      - warn
      - 2
   quotes:
      - error
      - single
   semi:
      - error
      - always
   no-console: 0
   no-unused-vars:
      - warn
plugins:
   - prettier
